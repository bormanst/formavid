{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/advagg": "^3.5",
        "drupal/advanced_help": "^1.0@alpha",
        "drupal/background_image": "^1.3",
        "drupal/backup_migrate": "^4.0",
        "drupal/captcha": "^1.0@beta",
        "drupal/components": "^1.0",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.6.0",
        "drupal/ctools": "^3.1",
        "drupal/devel": "^2.0",
        "drupal/features": "^3.8",
        "drupal/field_group": "^1.0",
        "drupal/fivestar": "1.x-dev",
        "drupal/honeypot": "^1.29",
        "drupal/image_style_quality": "^1.3",
        "drupal/imageapi_optimize": "^2.0@alpha",
        "drupal/imagemagick": "^2.3",
        "drupal/imce": "^1.7",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/module_filter": "^3.1",
        "drupal/panels": "^4.3",
        "drupal/pathauto": "^1.3",
        "drupal/recaptcha": "^2.4",
        "drupal/rules": "^3.0@alpha",
        "drupal/search_api_solr": "^2.5",
        "drupal/search_api_solr_defaults": "^2.5",
        "drupal/tagadelic": "^3.0@alpha",
        "drupal/views_bulk_operations": "^2.4",
        "drupal/zen": "^7.0@alpha",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.6.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
      "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
      "pre-install-cmd": [
          "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
      ],
      "pre-update-cmd": [
          "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
      ],
      "post-install-cmd": [
          "@drupal-scaffold",
          "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
      ],
      "post-update-cmd": [
          "@drupal-scaffold",
          "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
      ]
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
