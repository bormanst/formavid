# regenerate debian-sys-maint mysql password

PASSWORD=""

if [ -f /etc/mysql/debian.cnf ]; then
    ${FORMAVID}/bin/initialize/python/mysqlconf.py --user debian-sys-maint --pass="${PASSWORD}"
fi

exit 0
